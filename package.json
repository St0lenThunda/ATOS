{
  "name": "atos",
  "version": "0.0.1",
  "description": "A Thought Organization System",
  "productName": "ATOS",
  "author": "Antonio Moses",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn run server\" \"yarn run server2 \" \"yarn run server3 \" \"npm run dev\"",
    "server": "json-server -w ./src/assets/material-icons.json",
    "server2": "json-server -w -p 3001 ./backend/data.json",
    "server3": "json-server -w -p 3002 ./backend/data3.json",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar d",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.2.1",
    "json-server": "^1.0.0-beta.3",
    "jsonpath": "^1.1.1",
    "liquor-tree": "^0.2.70",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vanilla-jsoneditor": "^3.3.1",
    "vue": "^3.4.18",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-mdx-gfm": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/builder-vite": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/vue3": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@vitest/browser": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "@vue/devtools": "^7.7.2",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "playwright": "^1.52.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "storybook": "^8.6.12",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/St0lenThunda/ATOS.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/St0lenThunda/ATOS/issues"
  },
  "homepage": "https://github.com/St0lenThunda/ATOS#readme",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  }
}
